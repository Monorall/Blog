<h1>New Post</h1>

<%= form_for @post, html: { multipart: true } do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>

  <%= f.label :description %>
  <%= f.text_area :description %>

  <%= f.label :images %>
  <%= f.file_field :images, multiple: true %>

  <%= f.submit %>
<% end %>

<%= link_to 'Back', posts_path %>
